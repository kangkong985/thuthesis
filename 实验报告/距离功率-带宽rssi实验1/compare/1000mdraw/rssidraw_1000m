#set data style lines
set surface 
set contour surface

set view 60, 30, 1, 1
set clabel '%8.2f'
set key left
set title "rssi1000m"
set xlabel "power2"
set ylabel "power1"
set zlabel "rssi"

set xrange [300:1700]
set yrange [300:1700]

set term gif
set output "rssi1000m.gif"
set dgrid3d 15 15
splot "4-1000m.txt" using 5:4:15 notitle with pm3d

